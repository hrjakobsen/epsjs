{
  "name": "epsjs",
  "version": "0.0.1",
  "description": "A javascript library for rendering an EPS file into a canvas",
  "main": "dist/epsjs.mjs",
  "types": "dist/epsjs.d.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "dev": "webpack serve --mode=development",
    "build": "webpack build --mode=production",
    "lint": "eslint .",
    "generate-types": "dts-bundle-generator -o dist/epsjs.d.ts src/index.ts",
    "prepare": "npm run build && npm run generate-types"
  },
  "keywords": [
    "eps",
    "postscript",
    "ps",
    "pdf"
  ],
  "author": "Mathias Jakobsen",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "codemirror": "^6.0.1",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "9.4.2",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
